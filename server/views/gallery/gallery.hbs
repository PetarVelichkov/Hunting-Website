<div id="lightgallery">
    {{#each images}}
        <a href="/{{this.imgPath}}" class="col-lg-3 col-sm-4 col-xs-6">
            <img src="/{{this.imgPath}}" class="img-responsive img-thumbnail"/>
        </a>
    {{/each}}
</div>

<script type="text/javascript">
    $(document).ready(function() {
        $("#lightgallery").lightGallery();
    });
</script>